   src/database-metadata/generation/ora-dbmd.sql `
   example/ora/generated/dbmd.json `
   /Apps/dev/envs/ora-alis-dev.env

# generate queries for Oracle
npx ts-node src/query-generator-main.ts `
  $PWD/example/ora/generated/dbmd.json `
  $PWD/example/ora/query-specs.ts `
  example/ora/generated `
  example/ora/generated

# generate relation metadata for Postgres
npx ts-node src/relation-metadata-generator-main.ts `
  $PWD/example/ora/generated/dbmd.json `
