{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run Jest Tests",
        "type": "node",
        "request": "launch",
        "program": "${workspaceRoot}/node_modules/jest-cli/bin/jest.js",
        "stopOnEntry": false,
        "args": [ "--runInBand" ],
        "cwd": "${workspaceRoot}",
        "preLaunchTask": null,
        "runtimeExecutable": null,
        "runtimeArgs": [ "--nolazy" ],
        "env": { "NODE_ENV": "test" },
        "console": "integratedTerminal",
        "sourceMaps": true
      }
    ]
}