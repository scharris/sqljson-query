{
  "name": "sqljson-query",
  "version": "1.0.0",
  "description": "Command line tool to generate SQL/JSON queries and Typescript types for query results.",
  "keywords": [
    "SQL",
    "database"
  ],
  "author": "Stephen C. Harris",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scharris/sqljson-query.git"
  },
  "bin": {
    "sqljson-query-gen": "lib/cli.js"
  },
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "scripts": {
    "gen-qspecs-schema": "typescript-json-schema --refs --required --strictNullChecks -o lib/query-specs.schema.json tsconfig.json QueryGroupSpec",
    "gen-dbmd-schema": "typescript-json-schema --refs --required --strictNullChecks -o lib/dbmd.schema.json tsconfig.json DatabaseMetadataStoredProperties",
    "gen-schemas": "npm run gen-qspecs-schema && npm run gen-dbmd-schema",
    "build": "npm run clean && tsc && npm run gen-schemas",
    "test": "jest",
    "clean": "rimraf lib"
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.18",
    "@types/lodash": "^4.14.165",
    "@types/minimist": "^1.2.1",
    "@types/node": "^14.14.10",
    "copyfiles": "^2.4.1",
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lodash": "^4.17.20",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.3",
    "typescript-json-schema": "^0.45.0"
  }
}
