{
  "name": "sqljson-query",
  "version": "1.7.11",
  "description": "Command line tool to generate SQL/JSON queries and Typescript types for query results.",
  "keywords": [
    "SQL",
    "SQL/JSON",
    "database",
    "NO-ORM"
  ],
  "author": "Stephen C. Harris",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scharris/sqljson-query.git"
  },
  "main": "./lib/mod.js",
  "exports": "./lib/mod.js",
  "bin": {
    "gen-dbmd": "./lib/dbmd/generation/gen-dbmd.js",
    "gen-relsmd": "./lib/dbmd/generation/gen-relsmd.js"
  },
  "scripts": {
    "build-pg": "docker build -t drugs-pg src/__tests__/db/pg",
    "start-pg": "npm run build-pg && docker run -d --name drugs-pg --rm -p 127.0.0.1:5432:5432 --shm-size=256MB drugs-pg",
    "pg-cli": "docker exec -it drugs-pg psql -U drugs",
    "build-mysql": "docker build -t drugs-mysql src/__tests__/db/mysql",
    "start-mysql": "npm run build-mysql && docker run -d --name drugs-mysql --rm -p 127.0.0.1:3306:3306 drugs-mysql",
    "mysql-cli": "docker exec -it drugs-mysql mysql --user=drugs --password=drugs drugs",
    "rm-test-dbs": "docker rm -vf drugs-mysql drugs-pg",
    "start-test-dbs": "npm run start-pg && npm run start-mysql",
    "typecheck": "tsc --noEmit # (compile without emitting outputs)",
    "build": "npm run clean && tsc && copyfiles -u 2 src/dbmd/generation/**/*.* src/dbmd/generation/src/ lib/dbmd/",
    "test": "jest",
    "clean": "rimraf lib pack",
    "gen-doc-diagrams": "dot -O -Tsvg docs/img/*.dot"
  },
  "dependencies": {
    "ajv": "^8.10.0",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "zod": "^3.13.4"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/minimist": "^1.2.1",
    "@types/node": "^16.11.25",
    "@types/pg": "^8.6.4",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "mkdirp": "^1.0.4",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
